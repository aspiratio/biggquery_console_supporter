{
  "manifest_version": 3,
  "name": "BigQuery console supporter",
  "version": "1.0",
  "description": "Copy query-validation-status text from BigQuery Web Console.",
  "permissions": ["clipboardWrite"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://console.cloud.google.com/bigquery*"],
      "js": ["content.js"]
    }
  ]
}
